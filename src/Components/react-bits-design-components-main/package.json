{
  "name": "react-bits",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:registry && vite build",
    "build:registry": "npm run build:registry:default && npm run build:registry:tailwind",
    "build:registry:default": "npx jsrepo build --dirs ./src/content --output-dir ./public/default",
    "build:registry:tailwind": "npx jsrepo build --dirs ./src/tailwind --output-dir ./public/tailwind",
    "new:component": "node scripts/generateComponent.js",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@react-spring/web": "^9.7.4",
    "@react-three/drei": "^9.109.5",
    "@react-three/fiber": "^8.17.5",
    "framer-motion": "^11.3.24",
    "gsap": "^3.12.5",
    "postprocessing": "^6.36.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-haiku": "^2.1.8",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.26.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-use-gesture": "^9.1.3",
    "sass": "^1.77.8",
    "sonner": "^1.7.1",
    "three": "^0.167.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "jsrepo": "^1.26.2",
    "vite": "^5.3.4"
  }
}
